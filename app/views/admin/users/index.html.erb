<h1>Admin User Dashboard</h1>
<center class = 'grid-container'>
  <% @users.each do |user| %>
    <section class = 'user-snippet grid-item' id="user-<%= user.id %>">
      <%= link_to user.name, admin_user_path(user) %>
      <p>User Created At: <%= user.created_date %></p>
      <p>User Role: <%= user.role %></p>
    </section>
  <% end %>
</center>
